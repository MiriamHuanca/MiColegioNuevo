<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="cct.example.micolegio.PerfilE">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:text="DATOS PERSONALES DEL ESTUDIANTE"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView21l"
                android:textSize="16sp"
                android:textAlignment="center"
                android:layout_marginTop="25dp"
                android:layout_weight="1" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content">


                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@mipmap/ic_launcher"
                        android:id="@+id/imageView3" />
                </LinearLayout>

                <LinearLayout

                    android:orientation="vertical"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:text="Curso:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:lineSpacingExtra="18sp"
                            android:textSize="18sp"
                            android:layout_weight="1" />

                        <TextView
                            android:text=""
                            android:textAlignment="center"
                            android:background="#d6dcde"
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/mo_curso_e"
                            android:textSize="18sp"
                            android:layout_weight="1" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:text="Paralelo:"
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ji"
                            android:lineSpacingExtra="18sp"
                            android:textSize="18sp"
                            android:layout_weight="1" />

                        <TextView
                            android:text=""
                            android:background="#d6dcde"
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/mo_paralelo_e"
                            android:textSize="18sp"
                            android:layout_weight="1" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Nombres:"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView8"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_name_e"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Apellidos:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView10"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_surname_e"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="C.I.:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView12"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_ci_e"
                        android:textSize="16sp"
                        android:layout_weight="1" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Fecha Nacimiento:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView14"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_date_e"
                        android:textSize="16sp"
                        android:layout_weight="1" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Nacionalidad:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView16"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_nacionality_e"
                        android:textSize="16sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Género:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView18"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_gender_e"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Domicilio:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView20"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_domicile_e"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:text="DATOS PERSONALES DEL TUTOR:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView21"
                android:textSize="16sp"
                android:textAlignment="center"
                android:layout_marginTop="25dp"
                android:layout_weight="1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Nombres y Apellidos:"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView22"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_name_t"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="C.I.:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView24"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_ci_t"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>



                <TextView
                    android:text="Dirección de trabajo:"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:id="@+id/textView26"
                    android:textSize="18sp"
                    android:layout_weight="1" />

                <TextView
                    android:text=""
                    android:textAlignment="center"
                    android:background="#d6dcde"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/mo_direction_t"
                    android:textSize="18sp"
                    android:layout_weight="1" />


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Telefono / Celular:"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView28"
                        android:lineSpacingExtra="18sp"
                        android:textSize="18sp"
                        android:layout_weight="1" />

                    <TextView
                        android:text=""
                        android:textAlignment="center"
                        android:background="#d6dcde"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/mo_telephone_t"
                        android:textSize="18sp"
                        android:layout_weight="1" />
                </LinearLayout>

            </LinearLayout>



        </LinearLayout>
    </ScrollView>

</FrameLayout>
